pipeline {
    agent any

    stages {
        stage('Build'){
            steps {
                echo 'Building'
                echo 'hey'

            }
        }
    }
        stage('Test'){
            steps{
                bat 'set'
                sh 'echo "fail!"; exit1'
            }
        }
        stage('deploy'){
            steps{
             echo 'hey'
            }
        }
}
